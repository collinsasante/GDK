# Gospel Keys Demystified - Environment Variables Example
# Copy this file to .env and update the values

# Admin secret code for registration
REACT_APP_ADMIN_CODE=your_admin_code_here

# Environment (development/production)
REACT_APP_ENV=development

# App configuration
REACT_APP_NAME="Gospel Keys Demystified"
REACT_APP_VERSION=1.0.0

# Session duration in milliseconds (default: 24 hours)
REACT_APP_SESSION_DURATION=86400000

# Feature flags (true/false)
REACT_APP_ENABLE_BLOG=false
REACT_APP_ENABLE_EVENTS=false
REACT_APP_ENABLE_INSTRUCTORS=false
